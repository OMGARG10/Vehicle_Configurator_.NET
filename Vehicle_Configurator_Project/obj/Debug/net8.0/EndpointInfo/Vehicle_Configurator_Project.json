        ],
        "type": "object",
        "properties": {
          "mfgId": {
            "type": "integer",
            "format": "int32"
          },
          "mfgName": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "Model": {
        "type": "object",
        "properties": {
          "modelId": {
            "type": "integer",
            "format": "int32"
          },
          "modelName": {
            "type": "string",
            "nullable": true
          },
          "manufacturerId": {
            "type": "integer",
            "format": "int32"
          },
          "manufacturer": {
            "$ref": "#/components/schemas/Manufacturer"
          },
          "segmentId": {
            "type": "integer",
            "format": "int32"
          },
          "segment": {
            "$ref": "#/components/schemas/Segment"
          },
          "defaultComponents": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/VehicleDetail"
            },
            "nullable": true
          },
          "minQty": {
            "type": "integer",
            "format": "int32"
          },
          "price": {
            "type": "number",
            "format": "double"
          },
          "imagePath": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Segment": {
        "required": [
          "minQuantity",
          "segName"
        ],
        "type": "object",
        "properties": {
          "segId": {
            "type": "integer",
            "format": "int32"
          },
          "segName": {
            "minLength": 1,
            "type": "string"
          },
          "minQuantity": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "VehicleDetail": {
        "required": [
          "compType",
          "isConfigurable"
        ],
        "type": "object",
        "properties": {
          "configId": {
            "type": "integer",
            "format": "int32"
          },
          "componentId": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "component": {
            "$ref": "#/components/schemas/Component"
          },
          "compType": {
            "type": "string"
          },
          "isConfigurable": {
            "type": "string"
          }
        },
        "additionalProperties": false
